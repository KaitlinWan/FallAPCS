public class BigSib{
    public String helloMsg = "Hello ";

    public BigSib(String newHello){
	helloMsg = newHello;
	
	
    }

    public void setHelloMsg(String newMsg){
	
	helloMsg = newMsg;

    }
    
    public String greet(String name){

	String newString;
	
	newString = helloMsg;
	newString += " ";
	newString += name;

	return newString;

    } 


}
